<URLOperator>:
    size_hint_y: None
    height: 70
    md_bg_color: 1, 1, 1, 1
    pos_hint: {'center_x': .5, 'center_y': .5}
    MDBoxLayout:
        size_hint: .8, .6
        padding: [10, 0, 20, 0]
        radius: [10]
        pos_hint: {'center_x': 0.5, 'center_y': 0.35}
        #md_bg_color: app.theme_cls.accent_color
        canvas.before:
            Color:
                rgb: 204/255, 204/255, 204/255
            Line:
                width: 0.5
                rounded_rectangle: self.x, self.y, self.width, self.height, 8, 8, 8, 8, 100

        TextInput:
            id: user_password
            text: ''
            hint_text: "API URL"
            size_hint: .88, None
            pos_hint: {"center_x": .46, 'center_y': .5}
            height: self.minimum_height
            multiline: False
            cursor_width: "2sp"
            font_size: "12sp"
            password: True
            font_name: "fonts/Poppins-Regular.ttf"
            foreground_color: 204/255, 204/255, 204/255
            background_color: 0, 0, 0, 0
            #read_only: True
            #on_text: app.validate_hospital_id(*args)
            #on_focus: app.validate_when_focus(*args)